{
  "name": "@better-trino/client",
  "version": "0.0.5",
  "type": "module",
  "description": "A modern, type-safe TypeScript client for Trino with comprehensive error handling and full protocol support",
  "keywords": [
    "trino",
    "presto",
    "prestosql",
    "client",
    "typescript",
    "sql",
    "database",
    "query",
    "type-safe",
    "streaming"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nickBes/better-trino-client.git",
    "directory": "packages/better-trino-client"
  },
  "scripts": {
    "build": "tsdown --format esm --format cjs --dts",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.mts"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@better-trino/config": "workspace:*",
    "tsdown": "^0.20.0-beta.4",
    "typescript": "catalog:"
  }
